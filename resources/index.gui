  <text id="display-data-2" class="sensor-data" x="50%" y="160" font-size="24"></text>
  <text id="smallClock" class="time-data" x="50%" y="25">{ ... }</text>
  <text id="mainClock" class="mainClock" x="50%" y="150">{ ... }</text>
  <text id="data-label" class="time-data" visibility='visible' x="50%" y="240"></text>
  
