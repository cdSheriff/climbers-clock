    gradient-x2="0" gradient-y2="100%-50"
    gradient-color1="#161A1F" gradient-color2="#162d49" />
    <!-- TOP LEFT Loc-->
  <use id="btn-tl" href="#combo-button-upper-left" fill="#808000">
    <set href="combo-button-icon" attributeName="href" to="loc_line.png"/>
    <set href="combo-button-icon-press" attributeName="href" to="loc_full.png"/>
    <set href="combo-button-stroke" attributeName="display" to="inline"/>
  </use>
  <!-- TOP RIGHT Step-->
