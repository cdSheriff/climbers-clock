    <set href="combo-button-icon-press" attributeName="href" to="k2_full.png"/>
    <set href="combo-button-stroke" attributeName="display" to="inline"/>
  </use>
  <!-- BOTTOM LEFT CLOCK-->
  <use id="btn-bl" href="#combo-button-lower-left" fill="#CD853F">
    <set href="combo-button-icon" attributeName="href" to="heart_line.png"/>
    <set href="combo-button-icon-press" attributeName="href" to="heart_full.png"/>
<!--     <set href="combo-button-stroke" attributeName="display" to="inline"/> -->
  </use>
  
